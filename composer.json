    "keywords": ["openbase", "laravel", "laravel 5", "pdo_open", "Lumen"],
    "license": "MIT",
    "authors": [
        {
          "name": "Pedro Costa",
          "email": "pedro@plcosta.com"
        }
    ],
    "require": {
      "php": ">=5.5.9",
